#include <stdio.h>

int main(){
    int unidade, dezena, dezena5, centena;

    printf("Digite a quantidade de moedas de 1 real: ");
    scanf("%d", &unidade);

    printf("Digite a quantidade de notas de 10 reais: ");
    scanf("%d", &dezena);

    printf("Digite a quantidade de notas de 50 reais: ");
    scanf("%d", &dezena5);

    printf("Digite a quantidade de moedas de 100 reais: ");
    scanf("%d", &centena);

    int total = (unidade) + (dezena * 10) + (dezena5 * 50) + (centena * 100);

    printf("\nO total em dinheiro: %d reais", total);

    return 0;

}
